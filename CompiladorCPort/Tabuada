import java.util.Scanner;public class Tabuada {public static void main(String[] args){int a;int b;int c;System.out.print( "Digite um numero para tabuada:");Scanner teclado = new Scanner(System.in); a= teclado.nextInt();for(c=1;c<=10;c++){b = a * c;System.out.print( c);System.out.print( " x ");System.out.print( a);System.out.print( " = ");System.out.print( b);System.out.println();}}}